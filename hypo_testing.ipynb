    "def traverse_proc_directory(proc_directory):\n",
    "    all_proc_results = []\n",
    "\n",
    "    # Traverse the directory with PROC files\n",
    "    for root, dirs, files in os.walk(proc_directory):\n",
    "        for file in files:\n",
    "            proc_name = extract_job_and_proc_name(file)[0]  # Assuming first match is the proc name\n",
    "            file_path = os.path.join(root, file)\n",
    "\n",
    "            with open(file_path, 'r') as proc_file:\n",
    "                # Read content and filter lines\n",
    "                content_lines = [line for line in proc_file if pattern.match(line)]\n",
    "                content = \"\\n\".join(content_lines)\n",
    "\n",
    "                # Parse the content for program names and DSN/DISP values\n",
    "                proc_results = parse_jcl_content(content)\n",
    "\n",
    "                # Append results with proc name\n",
    "                for result in proc_results:\n",
    "                    all_proc_results.append({\n",
    "                        'proc_name': proc_name,\n",
    "                        'program_name_inside_proc': result['program_name'],\n",
    "                        'dsn_file_name': result['dsn_filename'],\n",
    "                        'disp_value': result['disp_value']\n",
    "                    })\n",
    "\n",
    "    # Convert the collected data into a DataFrame for better presentation\n",
    "    df = pd.DataFrame(all_proc_results)\n",
    "    return df\n",
    "\n",
    "# Example usage\n",
    "proc_directory = \"/path/to/proc/files\"\n",
    "proc_df = traverse_proc_directory(proc_directory)\n",
    "\n",
    "# Print the DataFrame\n",
    "print(proc_df)\n"
