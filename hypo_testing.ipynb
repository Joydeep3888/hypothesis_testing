    "def extract_job_and_proc_name(filename):\n",
    "    job_names = []\n",
    "    filename = filename.strip(\"'\")\n",
    "    pattern = re.compile(r'\\(([^()]+)\\)')\n",
    "    matches = pattern.findall(filename)\n",
    "    job_names.extend(matches)\n",
    "    return job_names"
